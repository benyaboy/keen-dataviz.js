{
  "presets": [
    ["env", {
      "production": {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7", "ie >= 11"]
        },
        "modules": false,
      },
      "development": {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7", "ie >= 11"]
        },
        "modules": false,
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
        ]
      }
    }]
  ],
  "plugins": ["transform-object-rest-spread"]
}
